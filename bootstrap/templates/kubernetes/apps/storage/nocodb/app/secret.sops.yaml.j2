---
apiVersion: v1
kind: Secret
metadata:
  name: nocodb
  namespace: storage
stringData:
  # Backend
  PORT: "8080"

  # Frontend
  NC_JWT_EXPIRES_IN: "48h"
  NC_PUBLIC_URL: "https://nocodb.{{}}"
  NC_DISABLE_TELE: "true"

  # Cache
  NC_REDIS_URL: ""
  
  # Postgres
  INIT_POSTGRES_DBNAME: "{{  }}"
  INIT_POSTGRES_HOST: "{{  }}"
  INIT_POSTGRES_USER: "{{  }}"
  INIT_POSTGRES_PASS: "{{  }}"
  INIT_POSTGRES_SUPER_PASS: "{{  }}"

  # Database
  NC_DB: "{{  }}"

  # Authentication
  NC_AUTH_JWT_SECRET: "{{  }}"
  NC_CONNECTION_ENCRYPT_KEY: "{{  }}"
  NC_ADMIN_EMAIL: "{{  }}"
  NC_ADMIN_PASSWORD: "{{  }}"

  # Storage
  NC_S3_ENDPOINT: "{{  }}"
  NC_S3_BUCKET_NAME: "{{  }}"
  NC_S3_FORCE_PATH_STYLE: "{{  }}"
  NC_S3_ACCESS_KEY: "{{  }}"
  NC_S3_ACCESS_SECRET: "{{  }}"

  # Email notification
  NC_SMTP_FROM: "{{  }}"
  NC_SMTP_HOST: "{{  }}"
  NC_SMTP_PORT: "{{  }}"
  NC_SMTP_USERNAME: ""
  NC_SMTP_PASSWORD: ""
  NC_SMTP_SECURE: "false"
  NC_SMTP_IGNORE_TLS: "true"